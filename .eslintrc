{
  "env": {
    "node": true,
    "jest": true
  },
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "no-tabs": 0,
    "indent": [2, "tab", {
			"SwitchCase": 1
		}],
    "object-curly-spacing": [2, "never"],
    "class-methods-use-this": 0,
    "no-console": 0,
    "no-await-in-loop": 0
  }
}
